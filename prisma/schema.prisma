// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./proxy.db"
}

generator client {
  provider = "prisma-client-js"
}

model BannedWebhook {
  id String @id

  reason String
}

model BannedIP {
  id String @id

  reason String
  expires DateTime
}

model BannedGame {
  id String @id

  reason String
}

model WebhooksSeen {
  id String @id
}